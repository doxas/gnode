{
  "source": "./src",
  "destination": "./docs",
  "plugins": [
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "manual": {
          "index": "./readme.md",
          "files": [
            "./manual/index.md",
            "./manual/codingstyle.md"
          ]
        }
      }
    }
  ]
}
